{
  "name": "@flitz/accept-language",
  "version": "1.0.0",
  "description": "Extracts information from Accept-Language header field and makes them available in request object of a flitz request.",
  "private": false,
  "main": "lib/index.js",
  "files": [
    "index.d.ts",
    "lib/**/*",
    "!lib/sandbox/**/*",
    "!**/*.map",
    "!**/__tests__/**/*.+(ts|tsx|js)",
    "!lib/setupTests.*"
  ],
  "scripts": {
    "build": "del ./lib && tsc -p ./tsconfig.json",
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "lint:fix": "eslint --fix -c .eslintrc.js --ext .ts src",
    "sandbox": "ts-node-dev --respawn ./src/sandbox/index.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flitz-js/accept-language.git"
  },
  "keywords": [
    "flitz",
    "accept",
    "language",
    "accept language",
    "header",
    "typescript"
  ],
  "author": "Marcel Joachim Kloubert <marcel.kloubert@gmx.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flitz-js/accept-language/issues"
  },
  "homepage": "https://github.com/flitz-js/accept-language#readme",
  "devDependencies": {
    "@flitz/tsconfig": "0.1.1",
    "@types/jest": "^26.0.8",
    "@types/node": "^10.17.28",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/eslint-plugin-tslint": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "del-cli": "^3.0.1",
    "eslint": "^7.5.0",
    "eslint-config-flitz": "0.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^30.1.0",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-unicorn": "^21.0.0",
    "flitz": "0.14.0",
    "jest": "^26.2.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.4",
    "ts-node-dev": "^1.0.0-pre.56",
    "tslint": "^6.1.3",
    "typescript": "^4.0.2"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "i18next": "^19.7.0"
  }
}
